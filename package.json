			},
			{
				"label": "_inkSea Theme Dracula - Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-dracula-soft.json"
			}
		]
	},
	"dependencies": {
		"js-yaml": "^3.13.1",
		"prettier": "^1.19.1",
		"tinycolor2": "^1.4.1",
		"vsce": "^1.79.5"
	}
