{
	"name": "inksea-theme",
	"displayName": "inkSea Theme",
	"description": "Easy on the Eyes, Soft on the Brain, Dark Pastel Theme",
	"version": "3.0.0",
	"publisher": "inksea",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/inksea.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/inksea/inksea-theme"
	},
	"engines": {
		"vscode": "^1.27.0-insider"
	},
	"categories": [
		"Themes"
	],
	"icon": "images/inksea-icon.png",
	"homepage": "https://github.com/inksea/inksea-theme",
	"gallerBanner": {
		"color": "#2a2c2d",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "_inkSea Theme",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea.json"
			},
			{
				"label": "_inkSea Theme - Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-soft.json"
			},
			{
				"label": "_inkSea Theme Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-dark.json"
			},
			{
				"label": "_inkSea Theme Dark - Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-dark-soft.json"
			},
			{
				"label": "_inkSea Theme Midnight",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-midnight.json"
			},
			{
				"label": "_inkSea Theme Midnight - Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-midnight-soft.json"
			},
			{
				"label": "_inkSea Theme Hacker",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-hacker.json"
			},
			{
				"label": "_inkSea Theme Hacker - Soft",
				"uiTheme": "vs-dark",
				"path": "./themes/inksea-hacker-soft.json"
